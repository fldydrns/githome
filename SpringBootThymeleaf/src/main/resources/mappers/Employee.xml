<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.neosoft.dao.EmployeeDao">
    <select id="findAll" resultType="jp.co.neosoft.entity.Employee">
        SELECT id, name, team, position, no, BIRTH, NATIONALITY FROM employee order by id
    </select>
</mapper>